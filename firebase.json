{
  "hosting": [{
    "target":"prod",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/api",
      "run": {
        "serviceId": "api",
        "region": "europe-west1" 
      }
    } ]
  }, {
    "target":"stg", 
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/api",
      "run": {
        "serviceId": "api",
        "region": "europe-west1" 
      }
    } ]
  }]
}
